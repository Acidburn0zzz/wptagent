{
  "manifest_version": 2,
  "name": "wptagent",
  "version": "1.0",
  "description": "Integrates with wptagent",
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
